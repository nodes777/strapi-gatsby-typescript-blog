{
  "name": "tayloredtotaylor-frontend",
  "description": "A photo blog for wildlife photography",
  "version": "1.0.0",
  "private": true,
  "author": "Taylor Nodell <taylor.nodell@gmail.com>",
  "keywords": [
    "gatsby",
    "iNaturalist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "clean-modules": "rmdir /s /q node_modules && yarn install",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "yarn run dev",
    "test": "yarn run type-check && yarn run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "10.3.1",
    "classnames": "2.2.6",
    "gatsby": "2.32.13",
    "gatsby-image": "2.4.12",
    "gatsby-plugin-canonical-urls": "2.3.9",
    "gatsby-plugin-emotion": "4.3.9",
    "gatsby-plugin-graphql-loader": "1.0.2",
    "gatsby-plugin-react-helmet-async": "1.1.0",
    "gatsby-plugin-sharp": "2.14.4",
    "gatsby-plugin-typescript": "2.12.1",
    "gatsby-remark-copy-linked-files": "2.3.10",
    "gatsby-remark-images": "3.11.1",
    "gatsby-remark-prismjs": "3.5.9",
    "gatsby-remark-responsive-iframe": "2.4.10",
    "gatsby-remark-smartypants": "2.3.9",
    "gatsby-source-strapi": "1.0.3",
    "gatsby-transformer-json": "2.4.10",
    "gatsby-transformer-remark": "2.16.1",
    "gatsby-transformer-sharp": "2.5.10",
    "modern-normalize": "0.5.0",
    "moment": "2.29.3",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-bottom-scroll-listener": "5.0.0",
    "react-dom": "16.8.6",
    "react-helmet-async": "1.0.7",
    "react-markdown": "4.3.1",
    "react-moment": "0.9.7",
    "typescript": "3.9.6"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "17.0.0-rc.2",
    "@types/classnames": "2.2.10",
    "@types/node": "12.12.47",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "5.0.16",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-config-blvd": "1.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "2.2.0",
    "prettier": "1.19.1",
    "rimraf": "2.7.1"
  },
  "resolutions": {"remark-mdx":"2.0.0-next.9"}

}
